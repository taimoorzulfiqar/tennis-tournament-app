{
  "expo": {
    "name": "Tennis Tournament App",
    "slug": "tennis-tournament-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#2E7D32"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#2E7D32"
      }
    },
    "web": {
      "bundler": "webpack",
      "output": "static",
      "template": "public/index.html"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "tennis-tournament-app",
    "sdkVersion": "53.0.0"
  }
}
